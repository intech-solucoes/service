{
    "compilerOptions": {
        "esModuleInterop": true,
        "lib": [
            "dom",
            "esnext"
        ],
        "skipLibCheck": true,
        "module": "commonjs",
        "outDir": "./dist",
        "strict": true,
        "target": "es5",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true
    }
}